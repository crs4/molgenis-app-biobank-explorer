server {
  server_name localhost;

  location /api {
    proxy_pass https://master.dev.molgenis.org;
    proxy_set_header Host master.dev.molgenis.org;
  }
  location / {
    root /usr/share/nginx/html;
    autoindex on;
    index       index.html;
    try_files   $uri $uri/ /index.html;
  }
  include /etc/nginx/sites-enabled/*;
}